<Window x:Class="Lunatic.TelescopeControl.Controls.GotoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lunatic.TelescopeControl.Controls"
        mc:Ignorable="d"
        Title="Goto Coordinate" Height="300" Width="300"
        Style="{DynamicResource PropertyDialogStyle}">
    <Grid>
        <DockPanel>
            <StackPanel DockPanel.Dock="Bottom">
                <Button Content="Goto" Command="{Binding GotoCommand}" />
            </StackPanel>
            <local:GotoControl />
        </DockPanel>
    </Grid>
</Window>
